<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<fx:root type="javafx.scene.layout.VBox" alignment="CENTER" spacing="20.0" prefWidth="900" prefHeight="900"
         xmlns:fx="http://javafx.com/fxml">
    <ImageView fx:id="logo"/>

    <Button maxHeight="100" maxWidth="200" text="Play !" onAction="#onPlayButtonClick"/>
    <Button maxHeight="100" maxWidth="200" text="Quit game" onAction="#onQuitButtonClick"/>
</fx:root>
